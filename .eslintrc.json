{
	"root": true,
	"extends": "wikimedia",
	"env": {
		"browser": true,
		"jquery": true,
		"qunit": true
	},
	"globals": {
		"OO": false,
		"mediaWiki": false,
		"require": false
	},
	"rules": {
		"dot-notation": [
			"error",
			{
				"allowKeywords": true
			}
		],
		"valid-jsdoc": 0,
		"indent": 0
	}
}
