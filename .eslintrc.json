{
	"root": true,
	"extends": [
		"wikimedia",
		"wikimedia/mediawiki"
	],
	"env": {
		"browser": true,
		"jquery": true,
		"qunit": true
	},
	"globals": {
		"mediaWiki": false
	},
	"rules": {
		"valid-jsdoc": 0,
		"indent": 0,
		"no-shadow": "warn",
		"mediawiki/msg-doc": "warn",
		"compat/compat": "warn"
	}
}
